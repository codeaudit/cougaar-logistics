  <ViewRefresh>120</ViewRefresh>
  <CompletionPSP>
   <URLConnection>
    <Host>sandwich.alpine.bbn.com</Host>
    <Port>8800</Port>
    <ClusterName>TRANSCOM</ClusterName>
    <Package></Package>
    <PspID>completion</PspID>
    <UseXML>N</UseXML>
   </URLConnection>
