<?xml version="1.0"  encoding="US-ASCII"?>
<!DOCTYPE Policy SYSTEM "Policy.ldm.dtd" []>
<Policies>
<!-- Inventory Policy for PackagedPOL at CONSUMERechelon of support -->
 <Policy name = "PackagedPOLPolicy" type="org.cougaar.logistics.plugin.inventory.InventoryPolicy">
    <RuleParam name="TransportTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="ResourceType">
        <String value = "PackagedPOL">
        </String>
    </RuleParam>
    <RuleParam name="OrderShipTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="FillToCapacity">
        <Boolean value = "false">
        </Boolean>
    </RuleParam>
    <RuleParam name="SupplierAdvanceNoticeTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="HandlingTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="SupplierArrivalTime">
        <String value = "08/25/2005 00:00">
        </String>
    </RuleParam>
    <RuleParam name="CriticalLevel">
        <Integer value = "3" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="ReorderPeriod">
        <Integer value = "3" min="0" max="1000">
        </Integer>
    </RuleParam>
 </Policy>
</Policies>
